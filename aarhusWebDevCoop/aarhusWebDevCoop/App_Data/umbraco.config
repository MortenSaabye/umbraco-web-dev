      <projectsItem id="1095" key="bf02df25-2ea0-4494-b2df-8e22f62cb393" parentID="1081" level="3" creatorID="0" sortOrder="2" createDate="2017-09-13T11:37:41" updateDate="2017-09-13T12:52:39" nodeName="Del 1: PaintToolSAI - Introduktion" urlName="del-1-painttoolsai-introduktion" path="-1,1078,1081,1095" isDoc="" nodeType="1066" creatorName="Morten Saabye Kristensen" writerName="Morten Saabye Kristensen" writerID="0" template="1065" nodeTypeAlias="projectsItem">
